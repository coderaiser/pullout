{
  "name": "pullout",
  "version": "1.0.2",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "pull out data from stream",
  "homepage": "http://github.com/coderaiser/pullout",
  "keywords": [
    "get",
    "pull",
    "body",
    "data",
    "stream"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/pullout.git"
  },
  "scripts": {
    "test": "tape test/*.js",
    "lint": "eslint lib test",
    "watch:test": "npm run watcher -- \"npm test\"",
    "watcher": "nodemon -w lib -w test -x",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "wisdom": "npm run build",
    "6to5": "buble lib -o legacy",
    "build": "redrun 6to5 legacy",
    "legacy": "echo \"module.exports = require('./pullout');\" > legacy/index.js"
  },
  "dependencies": {},
  "license": "MIT",
  "main": "lib/pullout.js",
  "devDependencies": {
    "buble": "^0.19.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "redrun": "^6.0.0",
    "tape": "^4.2.0"
  }
}
